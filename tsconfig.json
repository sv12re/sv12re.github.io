{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "moduleResolution": "node",
      "allowJs": true,
      "jsx": "react",
      "sourceMap": true,
      "outDir": "./generated",
      "strict": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "forceConsistentCasingInFileNames": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["./src/**.*"],
  }